{
	// Place your snippets for latex here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"include figure": {
		"prefix": "figure",
		"body": [
			"\\\\begin{figure}[htbp]",
			"\t\\\\centering",
			"\t\t\\\\includegraphics[height=6.0cm]{$1.eps}",
			"\t\t\\\\caption{$2}",
			"\t\t\\\\label{$3}",
			"\\\\end{figure}",
		],
		"description": "include graphics"
		},
		"include table": {
		"prefix": "table",
		"body": [
			"\\\\begin{table}[tb]",
			"\t\\\\centering",
			"\t\\\\caption{$1}",
			"\t\t\\\\begin{tabular}{c|c} \\\\hline",
			"\t\t\t$2&$3",
			"\t\t\\\\end{tabular}",
			"\\\\end{table}",
		],
		"description": "table"
	},
	"include double figure": {
		"prefix": "2figure",
		"body": [
			"\\\\begin{figure}[htbp]",
			"\t\\\\centering",
			"\t\t\\\\begin{tabular}{c}",
                  "\t\t\t\\\\begin{minipage}{0.50\\hsize}",
                    "\t\t\t\\\\centering",
                      "\t\t\t\\\\includegraphics[width=5.5cm]{$1.eps}",
                        "\t\t\t\t\\\\caption{$2}",
                        "\t\t\t\t\\\\label{$3}",
                  "\t\t\t\\\\end{minipage}",
                  "\t\t\t\\\\begin{minipage}{0.50\\hsize}",
                    "\t\t\t\\\\centering",
                      "\t\t\t\\\\includegraphics[width=5.5cm]{$1.eps}",
                        "\t\t\t\t\\\\caption{$2}",
                        "\t\t\t\t\\\\label{$3}",
                  "\t\t\t\\\\end{minipage}", 
                "\t\t\\\\end{tabular}  ",
			"\\\\end{figure}",
		],
		"description": "include two graphics"
		},
	"include cases in eqnarray": {
		"prefix": "cases&eqnarray",
		"body": [
			"\\\\begin{eqnarray}",
			"\t\\begin{cases}",
			"\t\t\\\\",
			"\t\\end{cases}",
			"\\\\end{eqnarray}",
		],
		"description": "cases in eqnarray"
	},
	"include eqnarray": {
		"prefix": "eqnarray",
		"body": [
			"\\\\begin{eqnarray}",
			"\t  &=&  \\\\",
			"\t  &=& ",
			"\\\\end{eqnarray}",
		],
		"description": "eqnarray"
	},
	"comment": {
		"prefix": "comment",
		"body": [
			"\\\\begin{comment}",
			"\t",
			"\\\\end{comment}",
		],
		"description": "comment"
	},
	"include tabular": {
		"prefix": "tabular",
		"body": [
			"\t\t\\\\begin{tabular}{c}",
			"\t\t\t$1",
			"\t\t\\\\end{tabular}",
		],
		"description": "tabular"
	}
}